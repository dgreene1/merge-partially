{
  "version": "2.0.2",
  "license": "MIT",
  "description": "A convenience method for overwriting only the values you want",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/dgreene1/merge-partially.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "update-toc": "markdown-toc -i README.md",
    "start": "tsdx watch",
    "build": "npm run update-toc && tsdx build",
    "test": "tsdx test",
    "test:coverage": "npm run test -- --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "prepare-badges": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "release-all": "np"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run test:coverage && tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "merge-partially",
  "author": "Dan Greene",
  "module": "dist/merge-partially.esm.js",
  "devDependencies": {
    "@types/clone-deep": "^4.0.1",
    "@types/faker": "^5.1.0",
    "coveralls": "^3.1.0",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "markdown-toc": "^1.2.0",
    "np": "^6.5.0",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "clone-deep": "^4.0.1",
    "utility-types": "^3.10.0"
  }
}
