{
  "name": "merge-partially",
  "version": "0.0.1",
  "description": "A convenience method for overwriting only the values you want",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "repository": "https://github.com/dgreene1/typeSafeJestSpy.git",
  "author": "Dan Greene",
  "license": "GPL-3.0",
  "files": [
    "build/src/**/*"
  ],
  "scripts": {
    "clean": "rimraf ./build && rimraf .tmp",
    "build": "npm run clean && tsc && npm run lint",
    "lint": "tslint \"**/*.ts\" --project .",
    "test": "jest",
    "prepare-badges": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "pre-commit": "npm run build && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "devDependencies": {
    "@types/clone-deep": "^4.0.1",
    "@types/jest": "^26.0.0",
    "coveralls": "^3.0.4",
    "husky": "^4.0.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "dependencies": {
    "clone-deep": "^4.0.1"
  }
}
